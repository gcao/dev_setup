Host subscribe-fe-vm-01.klga1.s.vonagenetworks.net
    ProxyCommand ssh -T prodlogin.vonagenetworks.net 'nc %h %p'
    HostName subscribe-fe-vm-01.klga1.s.vonagenetworks.net

